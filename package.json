{
  "name": "teospump",
  "version": "1.0.0",
  "private": true,
  "description": "Super advanced Next.js + TypeScript boilerplate.",
  "author": "Elmahrosa International",
  "repository": {
    "type": "git",
    "url": "https://github.com/Elmahrosa/TeosPump.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "postinstall": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.0",
    "axios": "^1.6.0",
    "styled-components": "^6.1.0",
    "next-seo": "^6.2.0",
    "dotenv": "^16.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.40",
    "@types/styled-components": "^5.1.30",
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@testing-library/react": "^14.2.0",  // Ensure this is correct
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.6.8",
    "env-cmd": "^10.1.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
